{
  "name": "Comet",
  "functions": [ ],
  "processes": [ {
    "name": "ReadDataSets",
    "function": "genXmlCollection",
    "ins": [ "xmlData", "config" ],
    "outs": [ "stations:dataSetsCount"]
  }, {
    "name": "PartitionData",
    "function": "partitionData",
    "ins": [ "stations" ],
    "outs": [ "dataParts" ]
  }, {
    "name": "ComputeStats",
    "function": "computeStats",
    "ins": [ "dataParts", "config" ],
    "parlevel": 3,
    "sticky": ["config"],
    "outs": [ "stats" ]
  }, {
    "name": "PlotGraphs",
    "function": "plotData",
    "parlevel": 3,
    "ins": [ "stats" ],
    "outs": [ "graph" ]
  }, {
     "name": "CollectPlots",
     "function": "collectGraphs",
     "ins": [ "graph:dataSetsCount" ],
     "outs": []
  } ],
  "signals": [ {
    "name": "xmlData",
    "data": [ { "path": "data.xml" } ]
  }, {
    "name": "counties"
  }, {
    "name": "stations"
  }, {
    "name": "timeWindow"
  }, {
    "name": "config",
    "data": [ { 
       "xpath": "//Collection[@label='station']",
       "start_time": "1.196499599E9",
       "end_time": "1.197359999E9",
       "baseTemp": "10"
    } ]
  }, {
    "name": "dataParts"
  }, {
    "name": "stats"
  }, {
    "name": "graph"
  }, {
    "name": "dataSetsCount",
    "control": "count"
  } ],
  "ins": [],
  "outs": []
}
