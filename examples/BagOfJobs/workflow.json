{
    "name": "BagOfJobsExample",
    "processes": [ {
        "name": "BojExecutor",
	"type": "dataflow",
	"firingLimit": 1,
        "function": "k8sCommand",
	"config": {
		"executor": {
			"image": "hyperflowwms/montage-workflow-worker",
			"executable": "echo",
			"args": ["Hello", "World"]
		}
	},
        "ins": [ "jobSetsFile" ],
        "outs": [ ]
    } ],
    "signals": [ {
        "name": "jobSetsFile",
        "data": [ "job_sets.json" ]
    } ],
    "ins": [ "jobSetsFile" ],
    "outs": [ ]
}
